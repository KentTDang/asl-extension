{
  "manifest_version": 3,
  "name": "My React Chrome Extension",
  "version": "1.0",
  "description": "A Chrome extension built with React",
  "action": {
    "default_title": "Open Side Panel"
  },
  "permissions": [
    "scripting",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
